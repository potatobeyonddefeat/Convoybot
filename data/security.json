{
  "enabled": true,
  "antiSpam": {
    "enabled": true,
    "windowMs": 400,
    "max": 900,
    "timeoutSeconds": 600
  },
  "badWords": {
    "enabled": true,
    "list": [
      "badword1",
      "badword2",
      "nigger"
    ],
    "delete": true,
    "warn": true,
    "censor": true,
    "maskChar": "*",
    "repostCensored": true
  },
  "links": {
    "enabled": true,
    "allowDomainList": [
      "discord.com",
      "discord.gg",
      "youtube.com",
      "youtu.be"
    ],
    "blockInvites": true
  },
  "mentions": {
    "enabled": true,
    "maxMentions": 5
  },
  "raidMode": {
    "enabled": true,
    "newAccountAgeDays": 30,
    "restrictNewMembers": true,
    "action": "timeout",
    "timeoutMinutes": 60,
    "massJoin": {
      "enabled": true,
      "threshold": 5,
      "windowMs": 60000,
      "lockdownMinutes": 10,
      "lockChannels": [
        "general"
      ]
    }
  }
}